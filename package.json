{
  "name": "@syu93/lit-router",
  "version": "0.2.1",
  "description": "LitRouter is a simple client side router, component loader (PRPL) based on page.js. It's designed to work well with Web Components and LitElement.",
  "keywords": [
    "webcomponents",
    "lit-element",
    "router",
    "prpl-pattern"
  ],
  "main": "pkg/dist-src/index.js",
  "module": "pkg/dist-src/index.js",
  "dependencies": {
    "page": "^1.11.4"
  },
  "devDependencies": {
    "@pika/pack": "^0.3.7",
    "@pika/plugin-build-web": "^0.3.16",
    "@pika/plugin-standard-pkg": "^0.3.16"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "scripts": {
    "build": "pack build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syu93/lit-router.git"
  },
  "author": "syu93",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syu93/lit-router/issues"
  },
  "homepage": "https://github.com/syu93/lit-router#readme"
}
